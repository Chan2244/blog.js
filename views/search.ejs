<div class="aurthor">
    <h1 class="author_heading">Search Results</h1>
</div>

<section class="articles">
    <ul class="aricle-ul">
        <% data.forEach(post => { %> 
        <li>
            <a href="/post/<%= post._id %>">
            <span><%= post.title %></span>
            < class="article-list_date"
                ><%= post.createdAt.toDateString() %></span>
            </a>
        </li>
        <% }) %>
    </ul>
</section>